package PageObjects;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class ManageClassPage {
	
	@FindBy(id="addClassName")
	public static WebElement addClassName;
	
	@FindBy(id="addClassOrder")
	public static WebElement addClassOrder;
	
	@FindBy(xpath="//span[text()='Section']")
	public static WebElement addSection;
	
	@FindBy(id="addClassTotalMark")
	public static WebElement addTotalMark;
	
	@FindBy(id="createClass")
	public static WebElement addBtnClk;
	
	@FindBy(xpath="(//i[@class='fa fa-times'])[1]")
	public static WebElement delectBtn;
	
	@FindBy(id="confirmDeleteClass")
	public static WebElement clkOkBtn;
	
	@FindBy(xpath="//h5[text()='Add Class']")
	public static WebElement actualMsg;
	

}
